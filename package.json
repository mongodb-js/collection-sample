{
  "name": "mongodb-collection-sample",
  "description": "Sample documents from MongoDB collections.",
  "version": "4.4.3",
  "scripts": {
    "check": "mongodb-js-precommit",
    "pretest": "mongodb-runner start --port=27018",
    "posttest": "mongodb-runner stop --port=27018",
    "test": "mocha --exit",
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt test/*.js bin/*.js index.js"
  },
  "precommit": [
    "check"
  ],
  "homepage": "https://github.com/mongodb-js/mongodb-collection-sample",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongodb-collection-sample.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mongodb-js/mongodb-collection-sample/issues"
  },
  "bin": {
    "mongodb-collection-sample": "bin/mongodb-collection-sample.js"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "event-stream": "^4.0.1",
    "lodash.chunk": "^4.0.0",
    "lodash.defaults": "^4.0.1",
    "reservoir": "^0.1.2",
    "semver": "^5.0.3"
  },
  "devDependencies": {
    "backoff": "^2.4.1",
    "bson": "^4.0.2",
    "chai": "^3.5.0",
    "eslint-config-mongodb-js": "^2.2.0",
    "lodash.range": "^3.0.1",
    "mocha": "^5.2.0",
    "mongodb": "^3.2.1",
    "mongodb-js-fmt": "0.0.3",
    "mongodb-js-precommit": "^0.2.9",
    "mongodb-read-preference": "^1.0.6",
    "mongodb-runner": "^4.6.0",
    "pre-commit": "^1.1.2",
    "proxyquire": "^1.7.9"
  },
  "optionalDependencies": {
    "event-stream": "^4.0.1",
    "mongodb-extjson": "^2.1.0",
    "mongodb-ns": "^2.0.0",
    "yargs": "^4.7.1"
  }
}
